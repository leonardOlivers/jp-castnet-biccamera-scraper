{
  "name": "jp-castnet-biccamera-scraper",
  "version": "1.0.0",
  "description": "Scrapes product data from Biccamera (JP) with pagination, concurrency, and JSON export.",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "test": "node -e \"console.log('No tests in this project tree.');\""
  },
  "keywords": [
    "scraper",
    "biccamera",
    "jp",
    "ecommerce",
    "crawler",
    "cheerio"
  ],
  "author": "Bitbash",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "engines": {
    "node": ">=18"
  }
}

